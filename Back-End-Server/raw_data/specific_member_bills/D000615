                "congress": "113",
                "bill_id": "hr3895-113",
                "bill_type": "hr",
                "number": "H.R.3895",
                "bill_uri": "https://api.propublica.org/congress/v1/113/bills/hr3895.json",
                "title": "To renew America's founding principles by freeing Americans to produce more energy in the United States from all sources and contribute to the strength of American national security through North American energy independence.",
                "short_title": "EXPAND Act",
                
                
                "sponsor_title": "Rep.",
                "sponsor_id": "D000615",
                "sponsor_name": "Jeffrey Duncan",
                "sponsor_state": "SC",
                "sponsor_party": "R",
                "sponsor_uri": "https://api.propublica.org/congress/v1/members/D000615.json",
                "gpo_pdf_uri": null,
                "congressdotgov_url": "https://www.congress.gov/bill/113th-congress/house-bill/3895",
                "govtrack_url": "https://www.govtrack.us/congress/bills/113/hr3895",
                "introduced_date": "2014-01-16",
                "active": false,
