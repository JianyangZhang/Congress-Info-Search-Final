                "gpo_pdf_uri": null,
                "congressdotgov_url": "https://www.congress.gov/bill/113th-congress/house-bill/3717",
                "govtrack_url": "https://www.govtrack.us/congress/bills/113/hr3717",
                "introduced_date": "2013-12-12",
                "active": false,
                "house_passage": null,
                "senate_passage": null,
                "enacted": null,
